{
  "name": "kova",
  "version": "1.2.2",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run prisma:db:push",
    "build": "ts-node scripts/version && ssr build",
    "release": "standard-version && git push --follow-tags origin master",
    "deploy": "ssr build && ssr deploy",
    "lint": "eslint . --ext .js,.tsx,.ts --cache",
    "lint:fix": "eslint . --ext .js,.tsx,.ts --cache --fix",
    "start": "ssr build && pm2 start pm2.config.js",
    "prisma:generate": "npx prisma generate",
    "prisma:db:push": "npx prisma db push",
    "analyze": "BUNDLE_ANALYZE=true npm run build",
    "dev": "ssr start",
    "seed": "ts-node prisma/seed.ts",
    "start:vite": "kova vite",
    "start:nest": "nest start --watch",
    "stop": "pm2 stop pm2.config.js",
    "postinstall": "node scripts/postbuild.ts",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.9",
    "@prisma/client": "^2.30.2",
    "@vue/compiler-sfc": "^3.2.0",
    "@vueuse/core": "^6.3.2",
    "ant-design-vue": "^2.2.6",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.10.6",
    "dplayer": "^1.26.0",
    "ioredis": "^4.27.9",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pinia": "^0.5.4",
    "pm2": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "ssr-core-vue3": "^5.6.17",
    "ssr-plugin-vue3": "^5.6.17",
    "swiper": "^7.0.4",
    "tencentcloud-sdk-nodejs": "^4.0.197",
    "vditor": "^3.8.6",
    "vue": "^3.2.11",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@kova/ssr": "^1.0.3",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^5.0.0",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
