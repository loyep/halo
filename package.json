    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "27.1.1",
    "less-vars-to-js": "^1.3.0",
    "prettier": "^2.3.2",
    "prisma": "^2.30.2",
    "prisma-dbml-generator": "^0.7.0",
